{
  "name": "smartstock",
  "description": "manage your sales, purchase and stock",
  "author": "SmartStock Company",
  "version": "21.7.6-build.12",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SmartStockTz/smartstock.git"
  },
  "build": {
    "appId": "com.fahamutech.smartstock",
    "mac": {
      "category": "Office"
    },
    "linux": {
      "category": "Office"
    },
    "win": {
      "icon": "build/icon.png",
      "target": [
        {
          "target": "appx",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "publish": "always"
  },
  "extraFiles": [
    "printer"
  ],
  "scripts": {
    "start": "electron .",
    "test": "ng test",
    "lint": "ng lint",
    "build:snap": "electron-builder --linux snap  --publish always",
    "build:mac": "electron-builder --mac  --publish always",
    "build:snap32": "electron-builder --linux --ia32 snap  --publish always",
    "build:win": "electron-builder --win  --publish always"
  },
  "dependencies": {
    "njre": "~0.2.0"
  },
  "devDependencies": {
    "electron": "^9.3.0",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0"
  },
  "private": true
}
